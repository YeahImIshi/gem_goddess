<h1><%= @category.title %></h1>

<ul>
  <% @products.each do |product| %>
    <li>
      <%= link_to product.title, product %>: <%= number_to_currency(product.price) %>
      <p><%= product.description %></p>
    </li>
  <% end %>
</ul>
<%= paginate @products %>

<%= link_to 'Back to Categories', categories_path %>